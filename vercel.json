{
    "version": 2,
    "builds": [
      {
        "src": "package.json", 
        "use": "vercel-sapper"
      },
      {
        "src": "queries.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {

        "src": "/(.*)",

        "dest": "index.js"
      },
      {

        "src": "/(.*)",

        "dest": "queries.js"
      }
    ]
  }